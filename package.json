  "dependencies": {
    "concurrently": "^7.0.0",
    "connect-livereload": "^0.6.1",
    "dotenv": "^16.0.0",
    "ejs": "3",
    "express": "^4.17.3",
    "livereload": "^0.9.3",
    "parcel": "^2.3.2"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.3.2",
    "@types/connect-livereload": "^0.6.0",
    "@types/dotenv": "8",
    "@types/ejs": "^3.1.0",
    "@types/express": "4",
    "@types/livereload": "^0.9.1",
    "@types/node": "16",
    "nodemon": "^2.0.15",
    "tslint": "^6.1.3",
    "typescript": "4"
  }
}
